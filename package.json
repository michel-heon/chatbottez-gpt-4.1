{
    "name": "gpt41",
    "version": "1.0.0",
    "msteams": {
        "teamsAppId": null
    },
    "description": "Microsoft 365 Agents Toolkit AI Chat Agent Sample with Teams AI Library",
    "engines": {
        "node": "18 || 20 || 22"
    },
    "author": "Microsoft",
    "license": "MIT",
    "main": "./lib/src/index.js",
    "scripts": {
        "dev:teamsfx": "env-cmd --silent -f .localConfigs npm run dev",
        "dev:teamsfx:testtool": "env-cmd --silent -f .localConfigs.playground npm run dev",
        "dev:teamsfx:launch-testtool": "env-cmd --silent -f env/.env.playground teamsapptester start",
        "dev": "nodemon --exec node --inspect=9239 --signal SIGINT -r ts-node/register ./src/index.ts",
        "build": "tsc --build && shx cp -r ./src/prompts ./lib/src",
        "start": "node ./lib/src/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:integration": "jest --testPathPattern=integration",
        "test:quota-load": "jest --testPathPattern=quota.*load",
        "test:db": "ts-node scripts/test-database.ts",
        "watch": "nodemon --exec \"npm run start\"",
        "db:migrate": "echo 'Run your database migrations here'",
        "db:seed": "echo 'Run your database seeding here'",
        "db:setup": "node -e \"console.log('Use: .\\\\scripts\\\\Setup-Database.ps1 (Windows) or ./scripts/setup-database.sh (Linux/Mac)')\"",
        "db:test": "npm run test:db"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com"
    },
    "dependencies": {
        "@microsoft/teams-ai": "^1.5.3",
        "botbuilder": "^4.23.1",
        "express": "^5.0.1",
        "uuid": "^9.0.1",
        "jsonwebtoken": "^9.0.2"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/node": "^18.0.0",
        "@types/uuid": "^9.0.7",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/jest": "^29.5.8",
        "@types/supertest": "^2.0.16",
        "env-cmd": "^10.1.0",
        "ts-node": "^10.4.0",
        "typescript": "^5.5.4",
        "nodemon": "^3.1.7",
        "shx": "^0.3.3",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "supertest": "^6.3.3"
    }
}